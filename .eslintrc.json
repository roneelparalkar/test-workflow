{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "Vue": "readable"
  },
  "extends": ["plugin:vue/essential", "plugin:vue/vue3-recommended"],
  // "extends": ["eslint:recommended", "plugin:vue/essential","plugin:vue/vue3-recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-console": ["warn", { "allow": ["error"] }]
  }
}
