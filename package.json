{
  "name": "final",
  "version": "6.0.1",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "GENERATE=1 nuxt generate",
    "serve:static": "cd dist/ && http-server -p 8080",
    "express": "NODE_ENV='development' node app.js",
    "server:build": "cd scripts/js && node create-server-build.js",
    "release": "standard-version",
    "upload:beta:build": "sh ./scripts/shell/upload-server-build.sh",
    "upload:prod:build": "sh ./scripts/shell/upload-server-build-prod.sh",
    "beta-deploy-time": "sudo npm run build && npm run server:build && npm run upload:beta:build",
    "prod-deploy-time": "sudo npm run build && npm run server:build && npm run upload:prod:build",
    "test": "mocha",
    "ingest-redis": "cd scripts/js/ && NODE_ENV=development node redis-ingestion-css-js.js",
    "lint-git-workflow": "eslint . ; exit 0",
    "lint": "eslint .",
    "lint-vue": "npx eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@nuxt/ufo": "^0.5.4",
    "@nuxtjs/axios": "^5.12.5",
    "cheerio": "^1.0.0-rc.5",
    "compression": "^1.7.4",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "deep-copy": "^1.4.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fast-json-parse": "^1.0.3",
    "helmet": "^4.4.1",
    "ioredis": "^4.22.0",
    "jsdom": "^16.5.3",
    "log-timestamp": "^0.3.0",
    "lru-cache": "^6.0.0",
    "luxon": "^1.26.0",
    "morgan": "^1.10.0",
    "nuxt": "^2.14.12",
    "nuxt-user-agent": "^1.2.2",
    "nuxt-winston-log": "^1.2.0",
    "promise.allsettled": "^1.0.4",
    "simdjson": "^0.5.0",
    "vee-validate": "2.1.7",
    "vue-datetime": "^1.0.0-beta.14",
    "vue-recaptcha-v3": "^1.9.0",
    "weekstart": "^1.0.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "artillery": "^1.6.2",
    "browserify": "^16.5.0",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "css-loader": "^5.1.2",
    "enforce-branch-name": "^1.0.1",
    "eslint": "^7.27.0",
    "eslint-plugin-vue": "^7.10.0",
    "fs-extra": "^8.1.0",
    "glider-js": "^1.7.7",
    "gulp": "^4.0.2",
    "gulp-string-replace": "^1.1.2",
    "gulp-uglify": "^3.0.2",
    "gulp-uglify-es": "^1.0.4",
    "husky": "^4.3.8",
    "import-fresh": "^3.3.0",
    "inquirer": "^7.3.3",
    "lazysizes": "^5.3.0",
    "lodash": "^4.17.20",
    "mobile-detect": "^1.4.5",
    "mocha": "^8.3.0",
    "request": "^2.88.2",
    "standard-version": "^9.1.1",
    "style-loader": "^2.0.0",
    "swiper": "^6.5.3",
    "ua-parser-js": "^0.7.24",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vue-infinite-scroll": "^2.0.2",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.41.1",
    "webpack-common-shake": "^2.1.0",
    "webpack-stream": "^5.2.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "pre-push-inside-husky-hooks": "npm run release",
  "post-merge-inside-husky-hooks": "npm run release"
}
